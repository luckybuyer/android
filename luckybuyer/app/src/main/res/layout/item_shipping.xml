<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:textSize="@dimen/dimen_14"
        android:textColor="@color/text_black"
        android:layout_marginTop="@dimen/dimen_16"
        android:layout_marginLeft="@dimen/dimen_12"
        android:text="name"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/iv_shipping_go"
        android:layout_width="@dimen/dimen_6"
        android:background="@drawable/go_go"
        android:layout_marginRight="@dimen/dimen_12"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dimen_24"
        android:layout_height="@dimen/dimen_11" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_toLeftOf="@+id/iv_shipping_go"
        android:text="13699140938"
        android:textSize="@dimen/dimen_14"
        android:textColor="@color/text_black"
        android:layout_marginTop="@dimen/dimen_22"
        android:layout_marginRight="@dimen/dimen_10"
        android:layout_height="wrap_content" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:background="@color/bg_view"
        android:layout_marginLeft="@dimen/dimen_12"
        android:layout_marginRight="@dimen/dimen_12"
        android:layout_below="@+id/iv_shipping_go"
        android:layout_marginTop="@dimen/dimen_10"
        android:layout_height="@dimen/dimen_1"></View>

    <net.luckybuyer.view.JustifyTextView
        android:text="yangshuyuyaohuanfangzile"
        android:layout_marginTop="@dimen/dimen_5"
        android:layout_below="@+id/view"
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/dimen_12"
        android:layout_marginRight="@dimen/dimen_12"
        android:maxLines="2"
        android:ellipsize="end"
        android:textSize="@dimen/dimen_12"
        android:textColor="#666666"
        android:layout_height="@dimen/dimen_34" />
</RelativeLayout>